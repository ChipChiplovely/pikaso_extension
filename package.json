{
  "name": "pikaso",
  "version": "1.0.0-beta.0",
  "description": "seamless and headless library for canvas",
  "main": "index.js",
  "keywords": [
    "canvas",
    "drawing",
    "graphic",
    "crop",
    "rotate",
    "shape",
    "draw",
    "flip",
    "text",
    "pencil",
    "konva",
    "fabric"
  ],
  "author": "Ramin Mousavi <ramin.mousavy@gmail.com> (http://github.com/raminious)",
  "license": "ISC",
  "browser": {
    "canvas": false,
    "jsdom": false
  },
  "bugs": {
    "url": "https://github.com/pikasojs/pikaso/issues"
  },
  "homepage": "https://github.com/pikasojs/pikaso",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/pikasojs/pikaso.git"
  },
  "scripts": {
    "typecheck": "tsc",
    "typedoc": "typedoc",
    "test": "jest",
    "_postinstall": "husky install",
    "prepublish": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "konva": "^7.1.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^5.0.0",
    "jest": "^26.6.2",
    "pinst": "^2.1.1",
    "prettier": "^2.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.19.2",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}